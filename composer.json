{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "artesaos/seotools": "^0.22.0",
        "atehnix/vk-client": "^1.4",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "box/spout": "^3.3",
        "cocur/slugify": "^4.1",
        "cviebrock/eloquent-sluggable": "7.0",
        "doctrine/dbal": "^3.0",
        "elgibor-solution/laravel-database-encryption": "^1.1",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "gdgrid/gd": "dev-master",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.7",
        "jenssegers/date": "^4.0",
        "laravel/framework": "^7.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "laravolt/avatar": "^4.1",
        "nicolaslopezj/searchable": "1.*",
        "qcod/laravel-imageup": "^1.0",
        "spatie/laravel-collection-macros": "^7.6",
        "spatie/laravel-mail-preview": "^4.0",
        "spatie/laravel-permission": "^5.5",
        "tightenco/ziggy": "^1.4",
        "vkcom/vk-php-sdk": "^5.101"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "laravel-lang/lang": "^10.5",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
